{
  "version": 2,
  "builds": [
    {
      "src": "src/main/resources/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main/resources/static/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/main/resources/static/index.html"
    }
  ]
}
